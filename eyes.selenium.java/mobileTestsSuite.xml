<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="passed tests" verbose="1" parallel="tests" thread-count="20">
    <test name="TestMobileDevicesIOS">
        <classes>
            <class name="com.applitools.eyes.selenium.TestMobileDevicesIOS">
                <methods>
                    <include name="TestIOSSafariStitch" />
                </methods>
            </class>
            <class name="com.applitools.eyes.selenium.TestIOSCrop">
                <methods>
                    <include name="TestIOSSafariCrop" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Test_Mobile">
        <classes>
            <class name="com.applitools.eyes.selenium.MobileNativeTests"/>
        </classes>
    </test>
    <test name="TestScrolling" parallel="methods">
        <parameter name="dont_run" value="TestWebAppScrolling+false, TestWebAppScrolling3+false"/>
        <classes>
            <class name="com.applitools.eyes.selenium.TestScrolling">
                <methods>
                    <include name="TestWebAppScrolling" />
                    <include name="TestWebAppScrolling2" />
                    <include name="TestWebAppScrolling3" />
                </methods>
            </class>
        </classes>
    </test>
</suite>
